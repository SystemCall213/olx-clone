{
    "version": 2,
    "builds": [
        { "src": "./index.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "headers": {
                "Access-Control-Allow-Origin": "https://olx-clone-three.vercel.app"
            }
        }
    ]
}