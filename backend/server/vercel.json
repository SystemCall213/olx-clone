{
    "version": 2,
    "builds": [
        { "src": "./index.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "headers": [
                { "key": "Access-Control-Allow-Origin", "value": "https://olx-clone-three.vercel.app" }
            ]
        }
    ]
}